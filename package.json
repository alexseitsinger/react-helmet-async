{
  "name": "@alexseitsinger/react-helmet-async",
  "version": "1.1.3",
  "description": "Thread-safe Helmet for React 16+ and friends",
  "main": "dist/main.js",
  "repository": "http://github.com/alexseitsinger/react-helmet-async",
  "author": "Scott Taylor <scott.c.taylor@mac.com>",
  "license": "Apache-2.0",
  "files": [
    "lib/",
    "src/",
    "index.d.ts"
  ],
  "dependencies": {
    "@babel/runtime-corejs3": "^7.7.2",
    "core-js": "^3.1.3",
    "invariant": "2.2.4",
    "prop-types": "15.7.2",
    "raf": "^3.4.1",
    "react-fast-compare": "2.0.4",
    "shallowequal": "1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.7.0",
    "@babel/core": "7.7.2",
    "@babel/plugin-proposal-class-properties": "7.7.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "10.0.3",
    "babel-loader": "^8.0.6",
    "babel-preset-kyt-react": "1.0.0-alpha.365f23ad",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.1",
    "enzyme-to-json": "3.4.3",
    "eslint": "6.6.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-kyt": "1.0.0-alpha.365f23ad",
    "eslint-config-prettier": "6.6.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "23.0.4",
    "eslint-plugin-json": "2.0.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-kyt": "1.0.0-alpha.6b63f166",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.16.0",
    "jest": "24.9.0",
    "jsdom": "15.2.1",
    "prettier": "1.19.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "rimraf": "^3.0.0",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-node-externals": "^1.7.2"
  },
  "peerDependencies": {
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  },
  "scripts": {
    "clean": "rimraf lib",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "test": "jest --no-cache",
    "test-watch": "npm run test -- --watch",
    "test-update": "npm run test -- -u",
    "build:development": "./node_modules/.bin/webpack --config=webpack.config.dev.js",
    "build:production": "./node_modules/.bin/webpack --config=webpack.config.js",
    "build": "yarn run build:development && yarn run build:production"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/src/",
      "/lib/",
      "/es/"
    ]
  }
}
